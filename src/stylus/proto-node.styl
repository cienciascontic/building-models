.proto-node
  *
    -webkit-touch-callout none
    -webkit-user-select none
    -khtml-user-select none
    -moz-user-select none
    -ms-user-select none
    user-select none
  padding 0px
  margin 0px
  border-radius 5px
  width 120
  height 160
  background-color none
  text-align center
  .img-background
    padding 4px
    border-radius 4px
    border 1px solid #bbb
    background-color white
    overflow hidden
  img
    max-width 110px
    max-height 110px
  .node-title
    font-face sans-serif
    padding 5px
    font-size 10pt
  
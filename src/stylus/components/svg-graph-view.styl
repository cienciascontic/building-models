.SvgGraphView
  position absolute
  svg
    path
      stroke-linecap round
      stroke light-blue
      fill none
      stroke-width 4px
      stroke light-blue
      &.axisLines
        stroke blue-gray
      &.data
        stroke gold

    text
      stroke-linecap round
      stroke none
      fill light-blue

      &.yLabel
        fill gold
        font-size 16
      &.xLabel
        fill green-blue
        font-size 16

  .unknown-graph
    position absolute
    font-size 2em
    top 43px
    left 68px

.canvas
  absolute(top: palette-and-actions-height + global-nav-height, left: 0, right: 0, bottom: 0)
  &.full
    top 0

.graph-view
  margin 0px
  position inherit
  width 100%
  height 100%
  background-color #ddd
  background-image url("../assets/img/bb-chrome/bg-horiz.png")
  overflow auto

  flex 10 0 200px
  align-self stretch

  -webkit-flex 10 0 200px
  -webkit-align-self stretch

  .label
    max-width: 100px
    text-align: center

.can-drop
  background-color gold

.container
  position absolute
  height 100%
  width 100%

.building-models
  padding 0px
  margin 0px
